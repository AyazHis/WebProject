@model IEnumerable<WebProject.Areas.Identity.Data.WPUser>


<div class="container-fluid">
   <div class="row pt-4">
       <div class="COL-6">
                   <h3 class="text primary">Пользователи</h3>
       </div>
   </div>

    @if(Model.Count() > 0)
    {
        <table class="table table-hover">
  <thead>
    <tr>
      <th scope="col" width="2%"># </th>
      <th scope="col" width="10%">E-MAIL</th>
      <th scope="col"width="15%">Номер телефона</th>
      <th scope="col" width="10%">Имя</th>
      <th scope="col" width="15%">Фамилия</th>
      <th scope="col">Город</th>
      <th scope="col">Улица</th>
      <th scope="col">Дом</th>
      <th scope="col" width="15%"> </th>

    </tr>
  </thead>
  <tbody>
      @foreach (var item in Model)
      {
      <tr>
      <th>АВТОПОДСЧЕТ!!</th>
      <td>@item.Email</td>
      <td>@item.PhoneNumber</td>
      <td>@item.FirstName</td>
      <td>@item.LastName</td>
      <td>@item.City</td>
      <td>@item.Street</td>
      <td>@item.House</td>
      <td><a asp-controller="Admin" asp-action="Edit" asp-route-Id="@item.Id" class="btn btn-outline-success btn-sm">Редактировать</a>
</td>
      </tr>
      }
  </tbody>
</table>
             }
    
  
</div>