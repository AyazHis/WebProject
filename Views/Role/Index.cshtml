@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>


 <h3>Роли</h3>
 <a asp-action="CreateRole">Создать</a>
 <table class="table table-striped table-bordered">
     <thead>
         <tr>
             <td>Id</td>
             <td>Name</td>
         </tr>
     </thead>
     <tbody>
         @foreach (var item in Model)
         {
             <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
         }
         
     </tbody>

 </table>